{
  "name": "zuzu",
  "version": "1.0.0",
  "type": "commonjs",
  "description": "A React scaffold application for integrating multiple tech stacks",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --config webpack.config.cjs --mode development --no-devtool --open",
    "build": "webpack --config webpack.config.cjs --mode production --no-devtool",
    "serve": "serve -s dist -l ${PORT:-3000}",
    "test": "cypress run",
    "test:open": "cypress open",
    "server": "ts-node --esm server/index.ts",
    "build:server": "cd server && npm install && tsc -p tsconfig.json",
    "server:dev": "ts-node --esm server/index.ts",
    "server:start": "node server/dist/index.js",
    "dev:server": "npm run build:server && npm run server:start",
    "prebuild": "webpack --config webpack.config.cjs --mode development",
    "dev": "concurrently \"npm run start\" \"npm run server:start\""
  },
  "keywords": [
    "react",
    "typescript",
    "mui",
    "redux",
    "tailwind",
    "webpack",
    "express",
    "tanstack",
    "cypress",
    "supabase"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^5.0.1",
    "@mui/icons-material": "^5.15.3",
    "@mui/material": "^5.15.3",
    "@mui/x-data-grid": "^7.28.3",
    "@reduxjs/toolkit": "^2.0.1",
    "@supabase/supabase-js": "^2.39.2",
    "@tanstack/react-query": "^4.36.1",
    "@tanstack/react-query-devtools": "^4.36.1",
    "browser": "^0.2.6",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "crypto-browserify": "^3.12.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "eventsource-parser": "^3.0.1",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "log4js": "^6.9.1",
    "morgan": "^1.10.1",
    "node-fetch": "^3.3.2",
    "os": "^0.1.2",
    "path": "^0.12.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-fetch": "^0.0.9",
    "react-hook-form": "^7.55.0",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.21.1",
    "serve": "^14.2.5",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/cypress": "^0.1.6",
    "@types/express": "^4.17.25",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "css-loader": "^6.8.1",
    "cypress": "^13.17.0",
    "dotenv-webpack": "^8.1.1",
    "html-webpack-plugin": "^5.6.3",
    "postcss": "^8.4.32",
    "postcss-loader": "^7.3.4",
    "react-bootstrap": "^2.10.9",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.4.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "wait-on": "^8.0.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
